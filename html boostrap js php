    <script>
        var projetos;
        var gostos;
        var comentarios;

        function selectCard(cardId) {
            var cards = document.getElementsByClassName('card-button');
            var totalImages = 9;
            var selectedCard = document.getElementById(cardId);



            // Verificando se o elemento foi encontrado
            if (selectedCard) {
                selecao = selectedCard.id;
                selecao = cardId.replace("cardButton", "");
                if (selecao <= 3) {
                    projetos = selecao;
                } else if (selecao <= 6) {
                    gostos = selecao;
                } else {
                    comentarios = selecao;
                }

                if (projetos == 2 && gostos == 4) {
                    gostos = 5;
                }


                if (projetos == 2 && comentarios == 7) {
                    comentarios = 8;
                }
            }

            // Remove 'active' class from all cards
            for (var i = 0; i < cards.length; i++) {
                cards[i].classList.remove('active');
            }

            // Add 'active' class to the selected card
            if (selectedCard) {
                selectedCard.classList.add('active');
            }

            // Reset filter for all images
            for (var i = 1; i <= totalImages; i++) {
                var image = document.getElementById("image" + i);
                if (image) {
                    image.style.filter = "invert(0)";
                }
            }

            if (projetos == 3) {
                document.getElementById("DivGostos").classList.add('d-none');
                document.getElementById("DivComentarios").classList.add('d-none');
            } else {
                document.getElementById("DivGostos").classList.remove('d-none');
                document.getElementById("DivComentarios").classList.remove('d-none');
            }

            // Apply filter to the selected image
            switch (cardId) {
                case "cardButton1":
                    if (document.getElementById("image1")) {
                        document.getElementById("image1").style.filter = "invert(1)";
                    }
                    break;
                case "cardButton2":
                    if (document.getElementById("image2")) {
                        document.getElementById("image2").style.filter = "invert(1)";
                    }
                    break;
                case "cardButton3":
                    if (document.getElementById("image3")) {
                        document.getElementById("image3").style.filter = "invert(1)";
                    }
                    break;
                case "cardButton4":
                    if (document.getElementById("image4")) {
                        document.getElementById("image4").style.filter = "invert(1)";
                    }
                    break;
                case "cardButton5":
                    if (document.getElementById("image5")) {
                        document.getElementById("image5").style.filter = "invert(1)";
                    }
                    break;
                case "cardButton6":
                    if (document.getElementById("image6")) {
                        document.getElementById("image6").style.filter = "invert(1)";
                    }
                    break;
                case "cardButton7":
                    if (document.getElementById("image7")) {
                        document.getElementById("image7").style.filter = "invert(1)";
                    }
                    break;
                case "cardButton8":
                    if (document.getElementById("image8")) {
                        document.getElementById("image8").style.filter = "invert(1)";
                    }
                    break;
                case "cardButton9":
                    if (document.getElementById("image9")) {
                        document.getElementById("image9").style.filter = "invert(1)";
                    }
                    break;
            }
        }

        function showLikes() {

            document.getElementById('rightPaneContent').innerHTML = `
            <div style="transition: 1s linear;">
                <div id="cardButton4" class="card card-button m-2 ${gostos == 4 ? "active" : ""} ${projetos == 2 ? "d-none" : ""}" onclick="selectCard('cardButton4')" style="height: 80px; transition: height 0.3s ease;">
                    <div class="card-body d-flex align-items-center" style="height: 80px;">
                        <img id="image4" ${gostos == 4 ? "style='filter: invert(1);'" : ""}  src="data:image/png;base64,">
                        <div class="ms-4">
                            <h5 class="card-title mb-1" style="margin-bottom: 0.25em;">Publico</h5>
                            <p class="card-text description">Qualquer pessoa pode ver...</p>
                        </div>
                    </div>
                </div>

                    <div id="cardButton5" class="card card-button m-2 ${gostos == 5 ? "active" : ""}" onclick="selectCard('cardButton5')">
                        <div class="card-body d-flex align-items-center" style="height: 80px;">
                            <img id="image5" ${gostos == 5 ? "style='filter: invert(1);'" : ""} src="data:image/png;base64,">
                            <div class="ms-4">
                                <h5 class="card-title mb-1">Apenas Seguidores</h5>
                                <p class="card-text description">Apenas quem lhe segue pode ver...</p>
                            </div>
                        </div>
                    </div>

                    <div id="cardButton6" class="card card-button m-2 ${gostos == 6 ? "active" : ""}" onclick="selectCard('cardButton6')">
                        <div class="card-body d-flex align-items-center" style="height: 75px;">
                            <img id="image6" ${gostos == 6 ? "style='filter: invert(1);'" : ""} src="data:image/png;base64,">
                            <div class="ms-4">
                                <h5 class="card-title mb-1">Apenas Eu</h5>
                                <p class="card-text description">Ninguém exceto você pode ver...</p>
                            </div>
                        </div>
                    </div>
            </div>
            `;
        }


        function showComments() {
            document.getElementById('rightPaneContent').innerHTML = `
                <div style="transition: 1s linear;">
                    <div id="cardButton7" class="card card-button m-2 ${comentarios == 7 ? "active" : ""} ${projetos == 2 ? "d-none" : ""}" onclick="selectCard('cardButton7')" style="height: 80px;">
                        <div class="card-body d-flex align-items-center" style="padding-top: 12px;">
                            <img id="image7" ${comentarios == 7 ? "style='filter: invert(1);'" : ""} src="data:image/png;base64,">
                            <div class="ms-4">
                                <h5 class="card-title mb-1" style="margin-bottom: 0.25em;">Publico</h5>
                                <p class="card-text description">Qualquer pessoa pode ver...</p>
                            </div>
                        </div>
                    </div>

                    <div id="cardButton8" class="card card-button m-2 ${comentarios == 8 ? "active" : ""}" onclick="selectCard('cardButton8')">
                        <div class="card-body d-flex align-items-center" style="height: 80px;">
                            <img id="image8" ${comentarios == 8 ? "style='filter: invert(1);'" : ""} src="data:image/png;base64,">
                            <div class="ms-4">
                                <h5 class="card-title mb-1">Apenas Seguidores</h5>
                                <p class="card-text description">Apenas quem lhe segue pode ver...</p>
                            </div>
                        </div>
                    </div>

                    <div id="cardButton9" class="card card-button m-2 ${comentarios == 9 ? "active" : ""}" onclick="selectCard('cardButton9')">
                        <div class="card-body d-flex align-items-center" style="height: 75px;">
                            <img id="image9" ${comentarios == 9 ? "style='filter: invert(1);'" : ""} src="data:image/png;base64,">
                            <div class="ms-4">
                                <h5 class="card-title mb-1">Apenas Eu</h5>
                                <p class="card-text description">Ninguém exceto você pode ver...</p>
                            </div>
                        </div>
                    </div>
                </div>
            `;
        }

        function showProject() {
            document.getElementById('rightPaneContent').innerHTML = `
                <div style="transition: 1s linear;">
                    <div id="cardButton1" class="card card-button m-2 ${projetos == 1 ? "active" : ""}"  onclick="selectCard('cardButton1')" style="height: 80px;">
                        <div class="card-body d-flex align-items-center" style="padding-top: 12px;">
                            <img ${projetos == 1 ? "style='filter: invert(1);'" : ""} id="image1" src="data:image/png;base64,">
                            <div class="ms-4">
                                <h5 class="card-title mb-1" style="margin-bottom: 0.25em;">Publico</h5>
                                <p class="card-text description">Qualquer pessoa pode ver...</p>
                            </div>
                        </div>
                    </div>

                    <div id="cardButton2" class="card card-button m-2 ${projetos == 2 ? "active" : ""}" onclick="selectCard('cardButton2')">
                        <div class="card-body d-flex align-items-center" style="height: 80px;">
                            <img ${projetos == 2 ? "style='filter: invert(1);'" : ""} id="image2" src="data:image/png;base64,">
                            <div class="ms-4">
                                <h5 class="card-title mb-1">Apenas Seguidores</h5>
                                <p class="card-text description">Apenas quem lhe segue pode ver...</p>
                            </div>
                        </div>
                    </div>

                    <div id="cardButton3" class="card card-button m-2 ${projetos == 3 ? "active" : ""}" onclick="selectCard('cardButton3')">
                        <div class="card-body d-flex align-items-center" style="height: 75px;">
                            <img ${projetos == 3 ? "style='filter: invert(1);'" : ""}  id="image3" src="data:image/png;base64,">
                            <div class="ms-4">
                                <h5 class="card-title mb-1">Apenas Eu</h5>
                                <p class="card-text description">Ninguém exceto você pode ver...</p>
                            </div>
                        </div>
                    </div>
                </div>
            `;
        }
    </script>
